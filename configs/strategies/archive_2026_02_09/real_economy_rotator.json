{
    "experiment_name": "real_economy_rotator",
    "processor": "generic",
    "model_type": "hybrid_v12",
    "use_gpu": true,
    "train_years": 13,
    "val_years": 2,
    "test_years": 2,
    "batch_size": 32,
    "epochs": 50,
    "early_stopping_patience": 10,
    "hidden_size": 256,
    "n_heads": 4,
    "dropout": 0.32,
    "lr": 0.00018,
    "lstm_layers": 2,
    "trans_layers": 7,
    "n_experts": 16,
    "loss_type": "focal",
    "extra_tickers": [
        "SPY",
        "USO",
        "XLI",
        "^OVX"
    ],
    "feature_pipeline": [
        {
            "step": "log_return",
            "input": "underlying_close",
            "output": "log_ret",
            "lags": [
                5,
                21
            ]
        },
        {
            "step": "arithmetic",
            "formula": "(underlying_close + uso) / (spy + 1e-6)",
            "output": "ratio_hard_soft"
        },
        {
            "step": "arithmetic",
            "formula": "xli / (spy + 1e-6)",
            "output": "ratio_industrial"
        },
        {
            "step": "normalize",
            "columns": [
                "ratio_hard_soft",
                "ratio_industrial",
                "vix",
                "^ovx"
            ],
            "window": 60,
            "suffix": "_norm"
        }
    ],
    "dynamic_columns": [
        "log_ret",
        "log_ret_5",
        "log_ret_21",
        "ratio_hard_soft_norm",
        "ratio_industrial_norm",
        "vix_norm",
        "^ovx_norm"
    ],
    "static_columns": [],
    "targets": [
        {
            "name": "target_1",
            "input": "underlying_close",
            "horizon": 5,
            "type": "log_future_return"
        },
        {
            "name": "target_2",
            "input": "underlying_close",
            "horizon": 21,
            "type": "log_future_return"
        }
    ],
    "lookback_window": 60,
    "optuna": {
        "trans_layers": [
            2,
            4,
            8
        ],
        "n_experts": [
            4,
            8,
            16
        ],
        "dropout": [
            0.2,
            0.4
        ]
    },
    "target_col": "target_21d",
    "input_dim": 7,
    "static_dim": 0
}