{
    "experiment_name": "v20_industrial_sniper_triple_barrier",
    "processor": "v20",
    "model_type": "experimental_v17",
    "use_revin": true,
    "bidirectional": true,
    "use_kan": true,
    "use_gpu": true,
    "use_directional_head": true,
    "train_years": 13,
    "val_years": 2,
    "test_years": 2,
    "batch_size": 32,
    "epochs": 150,
    "early_stopping_patience": 15,
    "hidden_size": 256,
    "n_heads": 8,
    "dropout": 0.17163436201621668,
    "lr": 0.0006323786437036849,
    "lstm_layers": 4,
    "trans_layers": 6,
    "n_experts": 4,
    "loss_type": "directional_focal",
    "dfl_alpha": 0.38006183311540365,
    "dfl_beta": 0.342240368819137,
    "dfl_gamma": 0.12604189909754335,
    "focal_gamma": 3.3046999342978056,
    "asymmetric_penalty": 5.508797927860238,
    "cls_weight": 0.5,
    "cls_threshold": 0.012306056344515795,
    "bear_weight": 4.423321811176969,
    "seq_length": 60,
    "extra_tickers": [
        "XLI",
        "XLB",
        "IWM",
        "DIA"
    ],
    "feature_pipeline": [
        {
            "step": "log_return",
            "input": "underlying_close",
            "output": "log_ret",
            "lags": [
                3,
                5,
                10,
                20
            ]
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "underlying_close",
            "period": 7,
            "output": "rsi_7"
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "underlying_close",
            "period": 14,
            "output": "rsi_14"
        },
        {
            "step": "technical_indicator",
            "indicator": "atr",
            "period": 5,
            "output": "atr_5"
        },
        {
            "step": "technical_indicator",
            "indicator": "atr",
            "period": 20,
            "output": "atr_20"
        },
        {
            "step": "arithmetic",
            "formula": "atr_5 / (atr_20 + 1e-6)",
            "output": "atr_ratio"
        },
        {
            "step": "bollinger_position",
            "input": "underlying_close",
            "window": 20,
            "output": "bb_pos"
        },
        {
            "step": "volume_ratio",
            "input": "volume",
            "window": 20,
            "output": "vol_ratio"
        },
        {
            "step": "close_vs_ma",
            "input": "underlying_close",
            "window": 7,
            "output": "close_vs_ma7"
        },
        {
            "step": "roc",
            "input": "underlying_close",
            "period": 3,
            "output": "roc_3"
        },
        {
            "step": "roc",
            "input": "underlying_close",
            "period": 7,
            "output": "roc_7"
        },
        {
            "step": "arithmetic",
            "formula": "xli / (xlb + 1e-6)",
            "output": "ratio_industrial"
        },
        {
            "step": "lag_diff",
            "input": "iwm",
            "lag": 5,
            "pct_change": true,
            "output": "iwm_mom_5"
        },
        {
            "step": "lag_diff",
            "input": "dia",
            "lag": 5,
            "pct_change": true,
            "output": "dia_mom_5"
        },
        {
            "step": "normalize",
            "columns": [
                "us_treasury_10y",
                "vix",
                "rsi_7",
                "rsi_14",
                "atr_ratio",
                "bb_pos",
                "vol_ratio",
                "close_vs_ma7",
                "roc_3",
                "roc_7",
                "ratio_industrial",
                "iwm_mom_5",
                "dia_mom_5"
            ],
            "window": 60,
            "suffix": "_norm"
        }
    ],
    "dynamic_columns": [
        "log_ret",
        "log_ret_3",
        "log_ret_5",
        "log_ret_10",
        "log_ret_20",
        "rsi_7_norm",
        "rsi_14_norm",
        "atr_ratio_norm",
        "bb_pos_norm",
        "vol_ratio_norm",
        "close_vs_ma7_norm",
        "roc_3_norm",
        "roc_7_norm",
        "ratio_industrial_norm",
        "iwm_mom_5_norm",
        "dia_mom_5_norm",
        "us_treasury_10y_norm",
        "vix_norm",
        "rsi_abs",
        "vix_abs"
    ],
    "static_columns": [
        "strike_distance",
        "dte_normalized",
        "is_call",
        "moneyness"
    ],
    "targets": [
        {
            "name": "target_1",
            "input": "underlying_close",
            "horizon": 7,
            "type": "log_future_return"
        },
        {
            "name": "target_2",
            "input": "underlying_close",
            "horizon": 21,
            "type": "log_future_return"
        }
    ],
    "input_dim": 20,
    "static_dim": 4
}