{
    "experiment_name": "deep_cycle_arbitrage_v19",
    "processor": "generic",
    "model_type": "experimental_v17",
    "use_revin": true,
    "bidirectional": true,
    "use_kan": true,
    "use_gpu": true,
    "use_directional_head": true,
    "train_years": 13,
    "val_years": 2,
    "test_years": 2,
    "batch_size": 128,
    "epochs": 150,
    "early_stopping_patience": 15,
    "hidden_size": 768,
    "n_heads": 8,
    "dropout": 0.21322629890088188,
    "lr": 8.68979926440654e-05,
    "lstm_layers": 9,
    "trans_layers": 6,
    "n_experts": 4,
    "loss_type": "directional_focal",
    "dfl_alpha": 0.345844181575479,
    "dfl_beta": 0.43991016023427654,
    "dfl_gamma": 0.24658564903057523,
    "focal_gamma": 2.7456627158221916,
    "asymmetric_penalty": 3.3045773192183816,
    "cls_weight": 0.5,
    "cls_threshold": 0.01410588611374938,
    "bear_weight": 3.514828813710736,
    "seq_length": 90,
    "extra_tickers": [
        "GLD",
        "TLT",
        "UUP",
        "EEM"
    ],
    "feature_pipeline": [
        {
            "step": "log_return",
            "input": "underlying_close",
            "output": "log_ret",
            "lags": [
                3,
                5,
                10,
                20,
                60
            ]
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "underlying_close",
            "period": 7,
            "output": "rsi_7"
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "underlying_close",
            "period": 14,
            "output": "rsi_14"
        },
        {
            "step": "technical_indicator",
            "indicator": "atr",
            "period": 5,
            "output": "atr_5"
        },
        {
            "step": "technical_indicator",
            "indicator": "atr",
            "period": 20,
            "output": "atr_20"
        },
        {
            "step": "arithmetic",
            "formula": "atr_5 / (atr_20 + 1e-6)",
            "output": "atr_ratio"
        },
        {
            "step": "bollinger_position",
            "input": "underlying_close",
            "window": 20,
            "output": "bb_pos"
        },
        {
            "step": "volume_ratio",
            "input": "volume",
            "window": 20,
            "output": "vol_ratio"
        },
        {
            "step": "close_vs_ma",
            "input": "underlying_close",
            "window": 7,
            "output": "close_vs_ma7"
        },
        {
            "step": "roc",
            "input": "underlying_close",
            "period": 3,
            "output": "roc_3"
        },
        {
            "step": "roc",
            "input": "underlying_close",
            "period": 7,
            "output": "roc_7"
        },
        {
            "step": "lag_diff",
            "input": "gld",
            "lag": 10,
            "pct_change": true,
            "output": "gld_mom_10"
        },
        {
            "step": "lag_diff",
            "input": "tlt",
            "lag": 10,
            "pct_change": true,
            "output": "tlt_mom_10"
        },
        {
            "step": "lag_diff",
            "input": "uup",
            "lag": 10,
            "pct_change": true,
            "output": "uup_mom_10"
        },
        {
            "step": "lag_diff",
            "input": "eem",
            "lag": 10,
            "pct_change": true,
            "output": "eem_mom_10"
        },
        {
            "step": "rolling_stat",
            "input": "log_ret",
            "window": 20,
            "stat": "skew",
            "output": "ret_skew_20"
        },
        {
            "step": "normalize",
            "columns": [
                "us_treasury_10y",
                "vix",
                "rsi_7",
                "rsi_14",
                "atr_ratio",
                "bb_pos",
                "vol_ratio",
                "close_vs_ma7",
                "roc_3",
                "roc_7",
                "gld_mom_10",
                "tlt_mom_10",
                "uup_mom_10",
                "eem_mom_10",
                "ret_skew_20"
            ],
            "window": 60,
            "suffix": "_norm"
        }
    ],
    "dynamic_columns": [
        "log_ret",
        "log_ret_3",
        "log_ret_5",
        "log_ret_10",
        "log_ret_20",
        "log_ret_60",
