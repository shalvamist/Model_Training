            "formula": "xly / (xlp + 1e-6)",
            "output": "ratio_risk"
        },
        {
            "step": "arithmetic",
            "formula": "xlk / (xlu + 1e-6)",
            "output": "ratio_growth"
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "ratio_risk",
            "period": 14,
            "output": "rsi_risk"
        },
        {
            "step": "technical_indicator",
