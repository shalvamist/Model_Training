{
    "experment_name": "sector_alpha_rotator_v18",
    "processor": "generic",
    "model_type": "experimental_v17",
    "use_revin": true,
    "bidirectional": true,
    "use_kan": true,
    "use_gpu": true,
    "train_years": 13,
    "val_years": 2,
    "test_years": 2,
    "batch_size": 32,
    "epochs": 60,
    "early_stopping_patience": 12,
    "hidden_size": 32,
    "n_heads": 4,
    "dropout": 0.47124994095006345,
    "lr": 0.00020147947767862594,
    "lstm_layers": 8,
    "trans_layers": 4,
    "n_experts": 8,
    "loss_type": "asymmetric_directional",
    "asymmetric_penalty": 3.0,
    "extra_tickers": [
        "XLY",
        "XLP",
        "XLK",
        "XLU"
    ],
    "feature_pipeline": [
        {
            "step": "log_return",
            "input": "underlying_close",
            "output": "log_ret",
            "lags": [
                5,
                20,
                60
            ]
        },
        {
            "step": "arithmetic",
            "formula": "xly / (xlp + 1e-6)",
            "output": "ratio_risk"
        },
        {
            "step": "arithmetic",
            "formula": "xlk / (xlu + 1e-6)",
            "output": "ratio_growth"
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "input": "ratio_risk",
            "period": 14,
            "output": "rsi_risk"
        },
        {
            "step": "technical_indicator",
            "indicator": "macd",
            "input": "ratio_growth",
            "output": "macd_growth"
        },
        {
            "step": "normalize",
            "columns": [
                "ratio_risk",
                "ratio_growth",
                "rsi_risk",
                "macd_growth",
                "us_treasury_10y",
                "vix"
            ],
            "window": 90,
            "suffix": "_norm"
        }
    ],
    "dynamic_columns": [
        "log_ret",
        "log_ret_5",
        "log_ret_20",
        "log_ret_60",
        "ratio_risk_norm",
        "ratio_growth_norm",
        "rsi_risk_norm",
        "macd_growth_norm",
        "us_treasury_10y_norm",
        "vix_norm"
    ],
    "static_columns": [
        "strike_distance",
        "dte_normalized",
        "is_call",
        "moneyness"
    ],
    "targets": [
        {
            "name": "target_1",
            "input": "underlying_close",
            "horizon": 5,
            "type": "log_future_return"
        },
        {
            "name": "target_2",
            "input": "underlying_close",
            "horizon": 21,
            "type": "log_future_return"
        }
    ],
    "lookback_window": 90,
    "target_col": "target_5d",
    "input_dim": 10,
    "static_dim": 4
}