{
    "processor_version": "generic",
    "feature_pipeline": [
        {
            "step": "log_return",
            "input": "underlying_close",
            "lags": [
                3,
                5,
                20,
                60
            ],
            "output": "log_ret"
        },
        {
            "step": "gap",
            "open_col": "underlying_open",
            "close_col": "underlying_close",
            "output": "gap"
        },
        {
            "step": "normalize",
            "columns": [
                "gap"
            ],
            "suffix": "_norm"
        },
        {
            "step": "lag_diff",
            "input": "underlying_close",
            "lag": 5,
            "pct_change": true,
            "output": "slope_5d"
        },
        {
            "step": "normalize",
            "columns": [
                "slope_5d"
            ],
            "suffix": "_norm"
        },
        {
            "step": "difference",
            "input": "slope_5d",
            "lag": 5,
            "output": "price_accel_5"
        },
        {
            "step": "normalize",
            "columns": [
                "price_accel_5"
            ],
            "suffix": "_norm"
        },
        {
            "step": "technical_indicator",
            "indicator": "rsi",
            "period": 14,
            "input": "underlying_close",
            "output": "rsi"
        },
        {
            "step": "rolling_stat",
            "input": "rsi",
            "window": 3,
            "stat": "mean",
            "output": "rsi_smooth"
        },
        {
            "step": "difference",
            "input": "rsi_smooth",
            "lag": 5,
            "output": "rsi_slope_5"
        },
        {
            "step": "lag_diff",
            "input": "vix",
            "lag": 5,
            "pct_change": true,
            "output": "vix_slope_5"
        },
        {
            "step": "difference",
            "input": "vix_slope_5",
            "lag": 5,
            "output": "vix_accel_5"
        },
        {
            "step": "normalize",
            "columns": [
                "vix"
            ],
            "suffix": "_norm"
        },
        {
            "step": "technical_indicator",
            "indicator": "atr",
            "period": 14,
            "output": "atr"
        },
        {
            "step": "arithmetic",
            "formula": "(atr / underlying_close) * 15.8745",
            "output": "iv"
        },
        {
            "step": "normalize",
            "columns": [
                "iv"
            ],
            "suffix": "_norm"
        },
        {
            "step": "greeks",
            "col_S": "underlying_close",
            "col_K": "underlying_close",
            "col_r": "us_treasury_10y",
            "col_sigma": "iv",
            "T_years": 0.1780821917808219,
            "rate_pct": true
        }
    ],
    "dynamic_columns": [
        "log_ret",
        "log_ret_3",
        "log_ret_5",
        "log_ret_20",
        "log_ret_60",
        "gap_norm",
        "slope_5d_norm",
        "price_accel_5_norm",
        "rsi_smooth",
        "rsi_slope_5",
        "delta",
        "gamma",
        "theta",
        "vega",
        "iv_norm",
        "rsi",
        "vix_norm",
        "vix_slope_5",
        "vix_accel_5"
    ],
    "targets": [
        {
            "horizon": 5,
            "name": "target_1"
        },
        {
            "horizon": 21,
            "name": "target_2"
        }
    ]
}